<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	<display-name></display-name>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>com.cqupt.deal.LoginServer</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>upload_table</servlet-name>
		<url-pattern>/send_table</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>upload_table</servlet-name>
		<servlet-class>com.cqupt.deal.ReceiveTableServer</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>record_items</servlet-name>
		<url-pattern>/get_record_items</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>record_items</servlet-name>
		<servlet-class>com.cqupt.deal.RecordItemsServer</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>record_item</servlet-name>
		<url-pattern>/get_record_item</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>record_item</servlet-name>
		<servlet-class>com.cqupt.deal.RecordItemServer</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>notify_items</servlet-name>
		<url-pattern>/get_notify_items</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>notify_items</servlet-name>
		<servlet-class>com.cqupt.deal.NotifyItemsServer</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>my_info</servlet-name>
		<url-pattern>/get_my_info</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>my_info</servlet-name>
		<servlet-class>com.cqupt.deal.MyInfoServer</servlet-class>
	</servlet>

</web-app>